<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity AspectMembership.
    -->
    <changeSet id="20170527190317-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="aspect_id"
                                 baseTableName="aspect_membership"
                                 constraintName="fk_aspect_membership_aspect_id"
                                 referencedColumnNames="id"
                                 referencedTableName="aspect"/>

        <addForeignKeyConstraint baseColumnNames="contact_id"
                                 baseTableName="aspect_membership"
                                 constraintName="fk_aspect_membership_contact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact"/>

        <addForeignKeyConstraint baseColumnNames="user_account_id"
                                 baseTableName="aspect_membership"
                                 constraintName="fk_aspect_membership_user_account_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_account"/>

    </changeSet>
</databaseChangeLog>
